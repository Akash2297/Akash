#include<iostream>
#include<iterator>
#include<algorithm>
#include<math.h>
#include<vector>
#include<stack>
#include<string.h>
#include<map>
#include<bitset>
#include<set>
#include<deque>
#include<sstream>
#include<queue>
#include<list>
#include <fstream>
#include <iomanip>
#define ln 1000005
#define pi acos(-1.0)
#define pb push_back
#define mp nake_pair
using namespace std;
int main()
{
    //std::ofstream outfile;
    //outfile.open("acm", std::ios_base::app);
    //outfile<<"write here"<<endl
    //outfile.close();
    
    int test_case,round=0;
    cin>>test_case;
    
    while(test_case--){
        round++;
        vector<int>akash;
        int n,k=0,r=0,c=0;
        cin>>n;
        
        //Input 2D Array and Finding k:
        int arr[n][n];
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                cin>>arr[i][j];
                
                if(i==j){
                    k+=arr[i][j];
                }
            }
        }
        //Finding r:
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                akash.pb(arr[i][j]);
            }
            sort(akash.begin(),akash.end());
            for(int k=0;k<n-1;k++){
                if(akash[k]==akash[k+1]){
                    r++;
                    break;
                }
            }
            akash.clear();
        }
        //Finding c:
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                akash.pb(arr[j][i]);
            }
            sort(akash.begin(),akash.end());
            for(int k=0;k<n-1;k++){
                if(akash[k]==akash[k+1]){
                    c++;
                    break;
                }
            }
            akash.clear();
        }
        cout<<"Case #"<<round<<": "<<k<<" "<<r<<" "<<c<<endl;
    }
    return 0;
}
