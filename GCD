#define pb push_back
#define mp make_pair
using namespace std;
void GCD(int number1,int number2){
    
    int temp1;
    
    if(number1<number2){
        int temp=number1;
        number1=number2;
        number2=temp;
    }
    
    while(true){
        
        temp1=number1%number2;
        
        if(temp1==0){
            cout<<"GCD : "<<number2<<"\n";
            break;
        }
        number1=number2;
        number2=temp1;
    }
}
int main()
{
    int number1,number2;
    
    while(scanf("%d %d",&number1,&number2)!=0){
    
    GCD(number1,number2);
        
    }
    
    return 0;
}
