#include<iostream>
using namespace std;
int main()
{
    printf("Enter the number of node : ");
    int node;
    scanf("%d",&node);
    
    printf("Enter the number of edge : ");
    int edge;
    scanf("%d",&edge);
    
    int adj[node][node];
    
    for(int i=0;i<node;i++){
        for(int j=0;j<node;j++){
            adj[i][j]=0;
        }
    }
    while(edge--){
        printf("Enter the graph : ");
        int a,b;
        scanf("%d %d",&a,&b);
        
        adj[a][b]=1;
        adj[b][a]=1;
    
    }
    for(int i=0;i<node;i++){
        for(int j=0;j<node;j++){
            printf("%d ",adj[i][j]);
        }
        printf("\n\n");
    }
    return 0;
}

